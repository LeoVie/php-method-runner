{
  "timeout": 10,
  "source": {
    "directories": [
      "src"
    ]
  },
  "phpUnit": {
    "configDir": "."
  },
  "logs": {
    "text": "build/logs/infection.log"
  },
  "mutators": {
    "@default": true,
    "UnwrapRtrim": {
      "ignore": [
        "LeoVie\\PhpMethodRunner\\Configuration\\Configuration::create"
      ]
    }
  },
  "tmpDir": "../cache"
}
